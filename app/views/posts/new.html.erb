<div class="container">
  <%= simple_form_for [@project, @post], html: { multipart: true } do |f| %>
    <%= f.input :title %>
    <%= f.input :media, as: :file, input_html: { multiple: true } %>
    <%= f.button :submit %>
  <% end %>

  <%= link_to "Back to project", project_path(@project) %>
</div>
